{
  "base": {
    "style": {
      "backgroundColor": "#F5F5F5",
      "borderColor": "#555555",
      "borderStyle": "solid",
      "borderWidth": "1px"
    }
  },
  "variants": [
    {
      "pseudo": "hover",
      "animation": {
        "duration": "0.1s",
        "direction": "normal",
        "iterationCount": "1",
        "timingFunction": "ease-in",
        "fillMode": "forwards",
        "keyframes": {
          "0%": {
            "backgroundColor": "#F5F5F5"
          },
          "100%": {
            "backgroundColor": "#D5D5D5"
          }
        }
      }
    }
  ]
}